{
  "name": "@eccenca/gui-elements",
  "description": "GUI elements based on other libraries, usable in React application, written in Typescript.",
  "version": "22.1.0",
  "license": "Apache-2.0",
  "homepage": "https://github.com/eccenca/gui-elements",
  "bugs": "https://github.com/eccenca/gui-elements/issues",
  "keywords": [
    "GUI",
    "React",
    "Blueprint",
    "Carbon Design System",
    "Typescript",
    "SCSS"
  ],
  "maintainers": [
    {
      "name": " eccenca GmbH",
      "email": "info@eccenca.com",
      "web": "https://eccence.com/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/eccenca/gui-elements.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=14.x"
  },
  "style": "src/index.scss",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:clean": "rimraf dist/",
    "build:esm": "tsc --project .typescript/tsbuild-esm.json && ./scripts/fix-esm-dist",
    "build:cjs": "tsc --project .typescript/tsbuild-cjs.json && ./scripts/fix-cjs-dist",
    "build:all": "yarn build:clean && yarn build:esm && yarn build:cjs",
    "compile": "tsc --build tsconfig.json",
    "compile-scss": "ts-node -O \"{ \\\"esModuleInterop\\\": true, \\\"module\\\": \\\"commonjs\\\"}\" ./scripts/compile-sass.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:ci": "jest --ci --reporters='default'",
    "test:coverage": "jest --collectCoverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "check:package": "pjv --warnings --recommendations"
  },
  "dependencies": {
    "@blueprintjs/colors": "^4.1.4",
    "@blueprintjs/core": "^4.6.1",
    "@blueprintjs/popover2": "^1.4.3",
    "@blueprintjs/select": "^4.5.1",
    "@carbon/icons": "10.44.0",
    "@carbon/icons-react": "10.44.0",
    "@mavrin/remark-typograf": "^2.2.0",
    "carbon-components": "10.44.0",
    "carbon-components-react": "7.44.1",
    "carbon-icons": "7.0.7",
    "codemirror": "^5.61.0",
    "color": "3.2.1",
    "lodash": "^4.17.21",
    "micromark": "^3.0.10",
    "re-resizable": "6.9.9",
    "react": "^16.13.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-flow-renderer": "9.7.4",
    "react-markdown": "^7.0.1",
    "rehype-raw": "^6.1.0",
    "remark-definition-list": "^1.1.0",
    "remark-gfm": "^3.0.0",
    "remark-parse": "^10.0.0",
    "reset-css": "^5.0.1",
    "unified": "^10.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.5",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-jest": "^6.4.13",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/carbon-components-react": "7.49.0",
    "@types/codemirror": "^5.60.5",
    "@types/color": "^3.0.2",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.5.1",
    "chromatic": "^6.5.1",
    "css-loader": "3.4.2",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-pnp-resolver": "^1.2.1",
    "node-sass-package-importer": "^5.3.2",
    "package-json-validator": "^0.6.3",
    "react-app-polyfill": "^1.0.6",
    "react-lorem-ipsum": "^1.4.9",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^10.3.1",
    "style-loader": "0.23.1",
    "ts-node": "^10.9.1",
    "tsc-esm-fix": "^2.13.0",
    "typescript": "4.4.4",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "resolutions": {
    "**/@carbon/icons": "10.44.0",
    "**/@carbon/icons-react": "10.44.0",
    "**/carbon-components": "10.44.0",
    "**/carbon-components-react": "7.44.1",
    "**/carbon-icons": "7.0.7",
    "**/@types/react": "^17.0.0",
    "**/node-gyp": "^7.0.0",
    "**/glob-parent": "^5.1.2",
    "**/trim": "^0.0.3",
    "**/trim-newlines": "^3.0.1",
    "**/minimist": "^1.2.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*(*.)@(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!react-markdown|vfile|unist-util-stringify-position|).+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
